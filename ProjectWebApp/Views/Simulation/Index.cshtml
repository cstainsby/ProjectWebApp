@{
}

@model IEnumerable<DataAccess.Models.SimulationProjectModel>

<h1>Simulation</h1>
<table class="table">
    <thead>
    </thead>

    <tbody>
        @foreach (var Simulation in ViewData["Simulation Data"] as IEnumerable<DataAccess.Models.SimulationProjectModel>)
        {
            <tr>
                <td>
                    <a class="btn text-black-50" asp-controller="SimulationPage" asp-action="SimulationPage">
                        @Simulation.simName
                    </a>
                </td>
            </tr>
        }
    </tbody>
</table>


